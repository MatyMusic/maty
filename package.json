{
  "name": "maty-music",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "env:check": "node -e \"require('dotenv').config({path:'.env.local'}); ['MONGODB_URI','NEXTAUTH_URL','NEXTAUTH_SECRET','GOOGLE_CLIENT_ID','GOOGLE_CLIENT_SECRET','ADMIN_EMAIL','SUPERADMINS','MONGODB_URI_NIGUNIM','MONGODB_DB_NIGUNIM','YOUTUBE_API_KEY','SPOTIFY_CLIENT_ID','SPOTIFY_CLIENT_SECRET','SPOTIFY_REFRESH_TOKEN'].forEach(k=>{console.log(process.env[k]?('✅ '+k):('❌ '+k+' missing'))})\"",
    "fit:ensure-indexes": "tsx -r dotenv/config -r tsconfig-paths/register src/scripts/fit-ensure-indexes.ts",
    "fit:import": "cross-env DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config -r tsconfig-paths/register src/scripts/fit-import.ts",
    "fit:import:wger": "cross-env DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config -r tsconfig-paths/register src/scripts/fit-import.ts --providers=wger --limit=5000",
    "fit:import:exercisedb": "cross-env DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config -r tsconfig-paths/register src/scripts/fit-import.ts --providers=exercisedb --limit=5000",
    "fit:import:ninjas": "cross-env DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config -r tsconfig-paths/register src/scripts/fit-import.ts --providers=apininjas --limit=4000",
    "fit:import:enrich": "cross-env DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config -r tsconfig-paths/register src/scripts/fit-import.ts --enrich=true --providers=wger,exercisedb,apininjas --limit=8000",
    "chat:server": "node -e \"console.log('Socket server disabled in this setup')\"",
    "clean": "node -e \"try{require('fs').rmSync('node_modules',{recursive:true,force:true})}catch(e){}; try{require('fs').rmSync('.next',{recursive:true,force:true})}catch(e){}; try{require('fs').rmSync('package-lock.json',{force:true})}catch(e){}\""
  },
